            },
            {
              "name": "campaignId",
              "value": "={{ $json.revivalCampaignId }}"
            },
            {
              "name": "sentAt",
              "value": "={{ $now.toISO() }}"
            }
          ]
        },
        "options": {}
      },
      "id": "log-email",
      "name": "Log Email Activity",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 4.1,
      "position": [1650, 150],
      "credentials": {
        "httpHeaderAuth": {
          "id": "3",
          "name": "CRM API"
        }
